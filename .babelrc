{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.0.0"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    ["prismjs", {
      "languages": ["javascript", "css"],
      "plugins": [
        "normalize-whitespace",
        "copy-to-clipboard"
      ],
      "theme": "tomorrow",
      "css": true
  }]
  ]
}
